{
  "name": "whatsapp-integrations",
  "version": "1.0.0",
  "description": "WhatsApp Integration using Evolution API",
  "type": "commonjs",
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start:prod": "node dist/core/webhook-server.js",
    "send": "tsx src/commands/send-message.ts",
    "check": "tsx src/commands/check-instance.ts",
    "create": "tsx src/commands/create-instance.ts",
    "qr": "tsx src/commands/get-qr.ts",
    "reconnect": "tsx src/commands/reconnect.ts",
    "webhook": "tsx src/core/webhook-server.ts",
    "webhook:log": "tsx src/core/webhook-server.ts 2>&1 | tee webhook.log",
    "start:all": "./start.sh",
    "stop:all": "./stop.sh",
    "setup-webhook": "tsx src/commands/setup-webhook.ts",
    "send-ocp": "tsx src/commands/send-ocp-message.ts",
    "bot-mode": "tsx src/features/set-bot-mode.ts",
    "agent-mode": "tsx src/features/set-agent-mode.ts",
    "restart-ocp": "tsx src/features/restart-ocp-session.ts",
    "start-conversation": "tsx src/features/start-conversation.ts",
    "dev": "tsx watch src/commands/send-message.ts"
  },
  "keywords": [
    "whatsapp",
    "evolution-api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ws": "^8.5.10",
    "@whiskeysockets/baileys": "^7.0.0-rc.9",
    "axios": "^1.6.0",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "form-data": "^4.0.5",
    "openai": "^4.104.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typescript": "^5.3.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0"
  }
}
